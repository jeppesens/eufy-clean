{
    "compilerOptions": {
      "lib": ["es2023"],
      "module": "node16",
      "target": "es2022",
      "rootDir": "src",
      "outDir": "out",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node16",
      "sourceMap": true,
      "allowJs": true,
    },
    "include": ["src**/*.ts", "src**/*.js", "main.ts"],
  }


